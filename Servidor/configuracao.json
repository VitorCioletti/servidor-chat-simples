{
    "WebSocket":
    {
        "Endereco": "127.0.0.1",
        "Porta": 5000
    },
    "Redis":
    {
        "Endereco": "127.0.0.1",
        "Porta": 6379
    },
    "Serilog": {
        "Using":  [ "Serilog.Sinks.Console" , "Serilog.Sinks.File"],
        "MinimumLevel": "Debug",
        "WriteTo": [
            {
                "Name": "File",
                "Args": {
                    "Path": "logs/log-{Date}.log",
                    "RollingInterval": "Day",
                    "RetainedFileCountLimit": 30,
                    "OutputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} - {Level:u} - {Message:lj}{NewLine}"
                }
            },
            {
                "Name": "Console",
                "Args": {
                    "OutputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} - {Level:u} - {Message:lj}{NewLine}"
                }
            }
        ]
    }
}
